<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="zdatbit.user.domain.User" table="user">
        <id name="userID" type="java.lang.Integer">
            <column name="userID" precision="11" scale="0" />
            <generator class="identity"/>
        </id>
        <property name="userName" type="java.lang.String">
            <column name="userName" length="50"/>
        </property> 
        <property name="userPassword" type="java.lang.String">
            <column name="userPassword" length="50"/>
        </property>
        <property name="salt2" type="java.lang.String">
            <column name="salt2" length="100"/>
        </property>
        <property name="realName" type="java.lang.String">
            <column name="realName" length="50"/>
        </property>
        <property name="userEmail" type="java.lang.String">
            <column name="userEmail" length="50"/>
        </property>
        <property name="userPhone" type="java.lang.String">
            <column name="userPhone" length="13"/>
        </property>
        <property name="company" type="java.lang.String">
            <column name="company" length="50"/>
        </property>
        <property name="department" type="java.lang.String">
            <column name="department" length="50"/>
        </property>
        <property name="userStatus" type="java.lang.Integer">
            <column name="userStatus" length="11"/>
        </property>
        <property name="isOnline" type="java.lang.Boolean">
            <column name="isOnline" />
        </property>
        <property name="lastLogin" type="java.util.Date">
            <column name="lastLogin" />
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="createTime"/>
        </property>

    </class>
</hibernate-mapping>
